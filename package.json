{
  "name": "admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "only-allow pnpm && sh ./scripts/preinstall.sh",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "depcheck": "depcheck",
    "mrm": "mrm",
    "coverage": "vitest run --coverage",
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "stylelint": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "format": "prettier .  --write",
    "changelog": "standard-version -t $(date +release-%Y%m%d-v)",
    "changelog:first": "standard-version --first-release -t $(date +release-%Y%m%d-v)"
  },
  "dependencies": {
    "@ant-design/icons-vue": "6.1.0",
    "@vueuse/components": "9.10.0",
    "@vueuse/core": "9.10.0",
    "@vueuse/router": "9.10.0",
    "ant-design-vue": "3.2.15",
    "axios": "1.2.2",
    "axios-retry": "3.3.1",
    "dayjs": "1.11.7",
    "mockjs": "1.1.0",
    "nanoid": "4.0.0",
    "pinia": "2.0.28",
    "vue": "3.2.45",
    "vue-i18n": "9",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.4.0",
    "@types/mockjs": "1.0.7",
    "@types/vue-i18n": "7.0.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@zougt/vite-plugin-theme-preprocessor": "1.4.8",
    "babel-plugin-import": "1.13.6",
    "commitlint": "17.4.0",
    "consola": "2.15.3",
    "depcheck": "1.4.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.8.0",
    "husky": "^8.0.0",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "lint-staged": "13.1.0",
    "mrm": "4.1.13",
    "only-allow": "1.1.1",
    "path": "0.12.7",
    "prettier": "2.8.2",
    "standard-version": "9.5.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "6.1.0",
    "typescript": "4.9.4",
    "unplugin-auto-import": "0.12.1",
    "unplugin-vue-components": "0.22.12",
    "vite": "4.0.4",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-inspect": "0.7.12",
    "vite-plugin-progress": "0.0.6",
    "vite-plugin-style-import": "2.0.0",
    "vitest": "0.26.3",
    "vue-tsc": "^1.0.11"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "pnpm run lint",
      "git add"
    ]
  },
  "license": "MIT"
}
